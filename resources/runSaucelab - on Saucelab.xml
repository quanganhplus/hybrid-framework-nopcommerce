<?xml version="1.0" encoding="UTF-8"?>
<suite name="Suacelab - Sort">

	<listeners>
		<listener class-name="commons.MethodListener"/>
	</listeners>
	
	<parameter name = "appUrl" value = "https://www.saucedemo.com/"/>

  <test name="Run on FIREFOX">
  	<parameter name="browser" value="Firefox"/>
  	<parameter name="osName" value="macOS 12" />
    <classes>
      <class name="fpt.nopcommerce.cloud.Level_19_Sort_Asc_Desc_Saucelab"/>
    </classes>
  </test> 
	
  <test name="Run on CHROME">
  	<parameter name="browser" value="Chrome"/>
  	<parameter name="osName" value="Windows 11" />
    <classes>
      <class name="fpt.nopcommerce.cloud.Level_19_Sort_Asc_Desc_Saucelab"/>
    </classes>
  </test> 
    
</suite> 
